-- GUI Creation
local ScreenGui = Instance.new("ScreenGui")
local Frame = Instance.new("Frame")
local ToggleButton = Instance.new("TextButton")
local EliminateButton = Instance.new("TextButton")

-- Parent the GUI to the game's PlayerGui
ScreenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
ScreenGui.Name = "ToggleGui"

-- Frame setup
Frame.Parent = ScreenGui
Frame.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
Frame.Position = UDim2.new(0.5, -100, 0.5, -50)
Frame.Size = UDim2.new(0, 200, 0, 100)
Frame.AnchorPoint = Vector2.new(0.5, 0.5)

-- Toggle Button setup
ToggleButton.Parent = Frame
ToggleButton.BackgroundColor3 = Color3.fromRGB(100, 100, 255)
ToggleButton.Position = UDim2.new(0.5, -70, 0.3, -15)
ToggleButton.Size = UDim2.new(0, 140, 0, 30)
ToggleButton.Text = "Turn On"
ToggleButton.TextColor3 = Color3.new(1, 1, 1)
ToggleButton.Font = Enum.Font.SourceSans
ToggleButton.TextSize = 24

-- Eliminate Button setup
EliminateButton.Parent = Frame
EliminateButton.BackgroundColor3 = Color3.fromRGB(255, 100, 100)
EliminateButton.Position = UDim2.new(0.5, -70, 0.7, -15)
EliminateButton.Size = UDim2.new(0, 140, 0, 30)
EliminateButton.Text = "Eliminate GUI"
EliminateButton.TextColor3 = Color3.new(1, 1, 1)
EliminateButton.Font = Enum.Font.SourceSans
EliminateButton.TextSize = 24

-- Toggle button functionality
local isOn = false
ToggleButton.MouseButton1Click:Connect(function()
    isOn = not isOn
    if isOn then
        ToggleButton.Text = "Turn Off"
        print("Feature is ON")
    else
        ToggleButton.Text = "Turn On"
        print("Feature is OFF")
    end
end)

-- Eliminate button functionality
EliminateButton.MouseButton1Click:Connect(function()
    ScreenGui:Destroy()
end)


